#, kde-format
msgid ""
"The new DB engine to use. Possible values are  \"sqlite\", \"mysql\" and "
"\"postgres\""
msgstr ""
"उपयोक्तुं नूतनं DB इञ्जिनम्। सम्भाव्यमानानि \"sqlite\", \"mysql\" \"postgres\" च सन्ति "
"।"

#: main.cpp:92
#, kde-format
msgctxt "@info:shell"
msgid "Invalid target engine: %1."
msgstr "अमान्य लक्ष्य इञ्जिन: %1."

#: main.cpp:106
#, kde-format
msgctxt "@info:status"
msgid "Migration completed successfully."
msgstr "प्रवासः सफलतया सम्पन्नः।"

#: main.cpp:108
#, kde-format
msgctxt "@info:status"
msgid "Migration failed."
msgstr "प्रवासः असफलः अभवत् ।"

#: main.cpp:113
#, kde-format
msgctxt "@info:progress"
msgid "Migrating table %1 (%2/%3)..."
msgstr "तालिका %1 (%2/%3) प्रवासयति..."

#: main.cpp:127
#, kde-format
msgctxt "@info:progress"
msgid "%1%..."
msgstr "%1%..."
