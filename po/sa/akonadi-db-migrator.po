#: dbmigrator.cpp:437
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "अकोनाडी सेवां स्थगयन्..."

#: dbmigrator.cpp:440
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "त्रुटिः: अकोनाडी इत्यस्य स्थगितस्य प्रतीक्षया समयसमाप्तिः ।"

#: dbmigrator.cpp:447
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "त्रुटिः: अकोनाडी कृते DBus तालं प्राप्तुं न शक्तवान्।"

#: dbmigrator.cpp:456
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "अकोनाडी सेवा आरभ्य..."

#: dbmigrator.cpp:469
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "दत्तांशकोश fsck सम्पन्न"

#: dbmigrator.cpp:483
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"अमान्यदत्तांशकोशइञ्जिन \"%1\" - वैधमूल्यानि \"sqlite\", \"mysql\" तथा \"postgres\" "
"सन्ति ।"

#: dbmigrator.cpp:505
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "त्रुटिः: स्रोतदत्तांशकोशसर्वरं विन्यस्तुं विफलम्।"

#: dbmigrator.cpp:510
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "स्रोतः गन्तव्यं च दत्तांशकोशइञ्जिनं समानम् अस्ति ।"

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr "त्रुटिः: स्रोतदत्तांशकोशदत्तांशसञ्चिकायाः निर्देशिकायाः वा मार्गं प्राप्तुं असफलम् ।"

#: dbmigrator.cpp:524
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "त्रुटिः: नूतनं दत्तांशकोशसर्वरं विन्यस्तुं असफलम्।"

#: dbmigrator.cpp:530
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr "त्रुटिः: दत्तांशं प्रवासयितुं विद्यमानं दत्तांशकोशं उद्घाटयितुं असफलम्।"

#: dbmigrator.cpp:535
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "त्रुटिः: दत्तांशं प्रवासयितुं नूतनं दत्तांशकोशं उद्घाटयितुं असफलम्।"

#: dbmigrator.cpp:540
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "स्रोतदत्तांशकोशे fsck चालयति"

#: dbmigrator.cpp:559
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "त्रुटिः: अस्थायीदत्तांशकोशनिर्देशिकां निष्कासयितुं असफलः %1: %2"

#: dbmigrator.cpp:566
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "मूलदत्तांशकोशस्य बैकअपं गृह्णन्..."

#: dbmigrator.cpp:574
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "बैकअप %1 तः %2 पर्यन्तं दत्तांशकोशस्य पुनर्स्थापनम्"

#: dbmigrator.cpp:576
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr "त्रुटिः: बैकअपतः दत्तांशकोशं पुनःस्थापयितुं असफलः: %1"

#: dbmigrator.cpp:608 dbmigrator.cpp:619
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "%1 सारणीं प्रवासयन् त्रुटिः अभवत्"

#: dbmigrator.cpp:638
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr "बैकअप निर्देशिका पूर्वमेव अस्ति । पूर्वस्य बैकअपस्य अधिलेखनं कर्तुम् इच्छति वा?"

#: dbmigrator.cpp:643
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "बैकअपं विना अग्रे गन्तुं न शक्यते। प्रवासः बाधितः।"

#: dbmigrator.cpp:648
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "पूर्वं बैकअप निर्देशिकां निष्कासयितुं असफलम् ।"

#: dbmigrator.cpp:661
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "दत्तांशकोशं बैकअपस्थानं प्रति स्थानान्तरयितुं असफलम्: %1"

#: dbmigrator.cpp:675
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "बैकअप सञ्चिका %1 पूर्वमेव अस्ति । अधिलेखनम् ?"

#: dbmigrator.cpp:677
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "बैकअपं विना अग्रे गन्तुं न शक्यते। प्रवासः बाधितः।"

#: dbmigrator.cpp:683
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "त्रुटिः: विद्यमानं बैकअपसञ्चिकां निष्कासयितुं असफलम् %1: %2"

#: dbmigrator.cpp:690
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Akonadi Server विन्यासस्य बैकअपं कर्तुं असफलम्: %1"

#: dbmigrator.cpp:707
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr "त्रुटिः: प्रवासितं दत्तांशकोशं प्राथमिकस्थानं प्रति स्थानान्तरयितुं असफलम्: %1"

#: dbmigrator.cpp:726
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "त्रुटि: मूल akonadiserverrc: %1 निष्कासयितुं असफलम्"

#: dbmigrator.cpp:732
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr "त्रुटिः: नूतनं akonadiserverrc प्राथमिकस्थानं प्रति स्थानान्तरयितुं असफलम्: %1"

#: dbmigrator.cpp:823
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "त्रुटिः: %1 सारणीयाः कृते स्वयम्वृद्धिमूल्यं अद्यतनीकर्तुं असफलम्"

#: dbmigrator.cpp:828
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "सारणी %1 अनुकूलनम्..."

#: dbmigrator.cpp:831
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "त्रुटिः: %1 सारणी अनुकूलितुं असफलम्"

#: main.cpp:31
#, kde-format
msgctxt ""
"Options for user to input to command line prompt, parenthesis indicate which "
"letter to type, capitalized option is default"
msgid " [(y)es/(N)o] "
msgstr " [(य)एस/(न)ओ] । "

#: main.cpp:36 main.cpp:55
#, kde-format
msgctxt "Letter for option \"(y)es\" prompted from command line"
msgid "y"
msgstr "तथा"

#: main.cpp:49
#, kde-format
msgctxt ""
"Options for user to input to command line prompt, parenthesis indicate which "
"letter to type, capitalized option is default"
msgid " [(y)es/(N)o/(s)kip] "
msgstr " [(य)एस/(न)ओ/(स)किप]। "

#: main.cpp:56
#, kde-format
msgctxt "Letter for option \"(s)kip\" prompted from command line"
msgid "s"
msgstr "स"

