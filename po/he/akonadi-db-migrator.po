#, kde-format
msgid ""
"The new DB engine to use. Possible values are  \"sqlite\", \"mysql\" and "
"\"postgres\""
msgstr ""
"מנגנון מסד נתונים חדש לשימוש. הערכים האפשריים הם „sqlite”,‏ „mysql” ו־"
"„postgres”"

#: main.cpp:92
#, kde-format
msgctxt "@info:shell"
msgid "Invalid target engine: %1."
msgstr "מנגנון היעד שגוי: %1"

#: main.cpp:106
#, kde-format
msgctxt "@info:status"
msgid "Migration completed successfully."
msgstr "ההסבה הושלמה בהצלחה."

#: main.cpp:108
#, kde-format
msgctxt "@info:status"
msgid "Migration failed."
msgstr "ההסבה נכשלה."

#: main.cpp:113
#, kde-format
msgctxt "@info:progress"
msgid "Migrating table %1 (%2/%3)..."
msgstr "הטבלה %1 עוברת הסבה (%2/%3)…"

#: main.cpp:127
#, kde-format
msgctxt "@info:progress"
msgid "%1%..."
msgstr "%1%…"
