#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "Fout: verwijderen van tijdelijke databasemap %1 is mislukt: %2"

#: dbmigrator.cpp:578
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "Reservekopie wordt gemaakt van originele database..."

