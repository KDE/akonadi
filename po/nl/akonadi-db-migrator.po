#: dbmigrator.cpp:650
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr ""
"Reservekopiemap bestaat al. Wilt u de vorige reservekopie overschrijven?"

#: dbmigrator.cpp:655
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Kan niet doorgaan zonder reservekopie. Migratie onderbroken."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Eerdere reservekopiemap verwijderen is mislukt."

#: dbmigrator.cpp:673
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "Database verplaatsen naar locatie voor reservekopie is mislukt: %1"

#: dbmigrator.cpp:687
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "Reservekopiebestand %1 bestaat reeds. Overschrijven?"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Kan niet doorgaan zonder reservekopie. Migratie onderbroken."

#: dbmigrator.cpp:695
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "Fout: verwijderen van bestaand reservekopiebestand %1 is mislukt: %2"

#: dbmigrator.cpp:702
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Reservekopie maken van Akonadi-serverconfiguratie is mislukt: %1"

#: dbmigrator.cpp:719
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Fout: gemigreerde database verplaatsen naar primaire locatie is mislukt: %1"

#: dbmigrator.cpp:738
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Fout: originele akonadiserverrc verwijderen is mislukt: %1"

#: dbmigrator.cpp:744
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Fout: nieuwe akonadiserverrc verplaatsen naar de primaire locatie is "
"mislukt: %1"

#: dbmigrator.cpp:835
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "Fout: waarde van autoincrement bijwerken voor table %1 is mislukt"

#: dbmigrator.cpp:840
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Tabel %1 wordt geoptimaliseerd..."

#: dbmigrator.cpp:843
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Fout: tabel %1 optimaliseren is mislukt"

