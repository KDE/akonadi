#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Akonadi-service wordt gestart..."

#: dbmigrator.cpp:481
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Fsck op database voltooid"

#: dbmigrator.cpp:495
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Ongeldige database-engine \"%1\" - geldige waarden zijn \"sqlite\", \"mysql"
"\" en \"postgres\"."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Fout: configureren van brondatabaseserver is mislukt."

#: dbmigrator.cpp:522
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "Bron en bestemmingsdatabase-engines zijn hetzelfde."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Fout: verkrijgen van pad naar brondatabasegegevensbestand of map is mislukt."

#: dbmigrator.cpp:536
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Fout: configureren van nieuwe databaseserver is mislukt."

#: dbmigrator.cpp:542
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr ""
"Fout: bestaande database openen om gegevens uit te migreren is mislukt."

#: dbmigrator.cpp:547
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "Fout: nieuwe database openen om gegevens naar te migreren is mislukt."

