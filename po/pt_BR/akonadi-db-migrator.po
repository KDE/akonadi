#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Iniciar serviço Akonadi..."

#: dbmigrator.cpp:468
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "fsck do banco de dados terminado"

#: dbmigrator.cpp:482
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Motor de banco de dados \"%1\" inválido - valores válidos são \"sqlite\", "
"\"mysql\" e \"postgres\"."

#: dbmigrator.cpp:504
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Erro: falha ao configurar a fonte do servidor de banco de dados."

#: dbmigrator.cpp:509
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "Fonte e destino dos motores de banco de dados são os mesmos."

#: dbmigrator.cpp:516
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Erro: falha ao obter o caminho para o arquivo ou diretório de fonte para os "
"dados do banco de dados."

#: dbmigrator.cpp:523
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Erro: falha ao configurar o novo servidor de banco de dados."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr ""
"Erro: falha ao abrir o banco de dados existente como origem da migração de "
"dados."

