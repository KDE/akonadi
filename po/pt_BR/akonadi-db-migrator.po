#: dbmigrator.cpp:436
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "Parando o serviço Akonadi..."

#: dbmigrator.cpp:439
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "Erro: tempo excedido ao esperar pela parada do Akonadi."

