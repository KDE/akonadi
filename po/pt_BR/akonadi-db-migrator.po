#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Falha ao remover a cópia de segurança anterior do diretório."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr ""
"Falha ao mover o banco de dados para a localização da cópia de segurança: %1"

#: dbmigrator.cpp:674
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "O arquivo  %1 de cópia de segurança já existe. Sobrescrever?"

#: dbmigrator.cpp:676
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr ""
"Não é possível continuar sem cópia de segurança. Migração interrompida."

#: dbmigrator.cpp:682
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr ""
"Erro: falha ao remover o arquivo existente %1: %2 de cópia de segurança"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr ""
"Falha ao realizar a cópia de segurança da configuração do Servidor Akonadi: "
"%1"

#: dbmigrator.cpp:706
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Erro: falha ao mover o banco de dados migrado para a localização primária: %1"

#: dbmigrator.cpp:725
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Erro: falha ao remover o akonadiserverrc original: %1"

#: dbmigrator.cpp:731
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Erro: falha ao mover o novo akonadiserverrc para a localização primária: %1"

#: dbmigrator.cpp:822
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "Erro: falha ao atualizar valor de incremento automático para tabela %1"

#: dbmigrator.cpp:827
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Otimizando tabela %1..."

#: dbmigrator.cpp:830
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Erro: falha ao otimizar a tabela %1"

