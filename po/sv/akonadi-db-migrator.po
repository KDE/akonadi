#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Databaskontroll klar"

#: dbmigrator.cpp:495
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Ogiltigt databasgränssnitt \"%1\" - giltiga värden är \"sqlite\", \"mysql\" "
"och \"postgres\"."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Fel: misslyckades ställa in källdatabasservern."

