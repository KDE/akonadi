#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "Säkerhetskopian %1 finns redan. Skriv över?"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Kan inte fortsätta utan säkerhetskopia. Konvertering avbruten."

#: dbmigrator.cpp:695
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "Fel: Misslyckades ta bort befintlig säkerhetskopia %1: %2"

#: dbmigrator.cpp:702
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Misslyckades säkerhetskopiera Akonadi-serverinställning: %1"

#: dbmigrator.cpp:719
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr "Fel: misslyckades flytta konverterad databas till primär plats: %1"

#: dbmigrator.cpp:738
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Fel: misslyckades ta bort ursprunglig akonadiserverrc: %1"

#: dbmigrator.cpp:744
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr "Fel: misslyckades flytta ny akonadiserverrc till primär plats: %1"

#: dbmigrator.cpp:835
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "Fel: misslyckades uppdatera automatiskt ökat värde för tabell %1"

#: dbmigrator.cpp:840
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Optimerar tabell %1"

#: dbmigrator.cpp:843
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Fel: misslyckades optimera tabell %1"

