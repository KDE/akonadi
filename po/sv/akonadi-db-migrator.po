#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Kan inte fortsätta utan säkerhetskopia. Konvertering avbruten."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Misslyckades ta bort tidigare katalog för säkerhetskopia."

#: dbmigrator.cpp:673
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
