#: dbmigrator.cpp:449
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "Detendo o servizo de Akonadi…"

#: dbmigrator.cpp:452
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "Erro: superouse o tempo límite agardando a que Akonadi se deteña."

#: dbmigrator.cpp:459
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "Erro: non foi posíbel obter o bloqueo de DBus para Akonadi."

#: dbmigrator.cpp:468
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Iniciando o servizo de Akonadi…"

# skip-rule: trasno-file_reverse, trasno-file-system_reverse
#: dbmigrator.cpp:481
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Completouse a comprobación do sistema de ficheiros da base de datos."

#: dbmigrator.cpp:495
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"O motor de base de datos «%1» non é válido. Os valores válidos son «sqlite», "
"«mysql» e «postgres»."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Erro: non foi posíbel configurar o servidor de base de datos de orixe."

#: dbmigrator.cpp:522
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "Os motores de base de datos de orixe e de destino coinciden."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Erro: non foi posíbel obter a ruta do ficheiro ou cartafol de datos da base "
"de datos de orixe."

#: dbmigrator.cpp:536
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Erro: non foi posíbel configurar o novo servidor de base de datos."

#: dbmigrator.cpp:542
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr ""
"Erro: non foi posíbel abrir unha base de datos existente da que migrar datos."

#: dbmigrator.cpp:547
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr ""
"Erro: non foi posíbel abrir unha nova base de datos á que migrar datos."

# skip-rule: trasno-file_reverse, trasno-file-system_reverse
#: dbmigrator.cpp:552
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr ""
"Executando a comprobación de sistema de ficheiros na base de datos de orixe."

#: dbmigrator.cpp:571
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr ""
"Erro: non foi posíbel retirar o cartafol «%1» de base de datos temporal: %2"

#: dbmigrator.cpp:578
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "Salvagardando a base de datos orixinal…"

#: dbmigrator.cpp:586
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "Restaurando a base de datos da salvagarda %1 en %2."

#: dbmigrator.cpp:588
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr "Erro: non foi posíbel restaurar a base de datos da salvagarda: %1"

#: dbmigrator.cpp:620 dbmigrator.cpp:631
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "Produciuse un erro ao migrar a táboa %1."

#: dbmigrator.cpp:650
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr ""
"O cartafol de salvagarda xa existe. Quere sobrescribir a salvagarda anterior?"

#: dbmigrator.cpp:655
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Non é posíbel proceder sen salvagarda. Interrompeuse a migración."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Non foi posíbel retirar o cartafol de salvagarda anterior."

#: dbmigrator.cpp:673
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "Non foi posíbel mover a base de datos á localización de salvagarda: %1"

#: dbmigrator.cpp:687
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "O ficheiro de salvagarda %1 xa existe. Quere sobrescribilo?"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Non é posíbel proceder sen salvagarda. Interrompeuse a migración."

#: dbmigrator.cpp:695
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr ""
"Erro: non foi posíbel retirar o ficheiro existente %1 de salvagarda: %2"

#: dbmigrator.cpp:702
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Non foi posíbel salvagardar a configuración do servidor de Akonadi: %1"

#: dbmigrator.cpp:719
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Erro: non foi posíbel mover a base de datos migrada á localización "
"principal: %1"

#: dbmigrator.cpp:738
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Erro: non foi posíbel retirar o «akonadiserverrc» orixinal: %1"

#: dbmigrator.cpp:744
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Erro: non foi posíbel mover o novo «akonadiserverrc» á localización "
"principal: %1"

#: dbmigrator.cpp:835
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr ""
"Erro: non foi posíbel actualizar o valor de aumento automático da táboa %1."

#: dbmigrator.cpp:840
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Optimizando a táboa %1…"

#: dbmigrator.cpp:843
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Erro: non foi posíbel optimizar a táboa %1."

