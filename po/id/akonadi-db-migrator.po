#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr "Galat: gagal memindah database yang termigrasi ke lokasi primer: %1"

#: dbmigrator.cpp:725
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Galat: gagal menghapus akonadiserverrc orisinal: %1"

#: dbmigrator.cpp:731
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr "Galat: gagal memindah akonadiserverrc baru ke lokasi primer: %1"

#: dbmigrator.cpp:822
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "Error: gagal memperbarui nilai autoincrement untuk tabel %1"

#: dbmigrator.cpp:827
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Mengoptimalkan tabel %1..."

#: dbmigrator.cpp:830
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Error: gagal mengoptimalkan tabel %1"

