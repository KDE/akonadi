#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Mesin database tidak valid \"%1\" - nilai valid adalah \"sqlite\", \"mysql\" "
"dan \"postgres\"."

