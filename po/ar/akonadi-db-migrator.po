#: dbmigrator.cpp:436
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "إيقاف خدمة أكونادي..."

#: dbmigrator.cpp:439
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "خطأ: انتهت المهلة أثناء انتظار توقف أكونادي."

#: dbmigrator.cpp:446
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "خطأ: تعذر الحصول على قفل DBus لأكونادي."

#: dbmigrator.cpp:455
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "بدء خدمة أكونادي..."

#: dbmigrator.cpp:468
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "اكتمل التحقق من اتساق نظام الملفات fsck لقاعدة البيانات"

#: dbmigrator.cpp:482
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"محرك قاعدة البيانات \"%1\" غير صالح - القيم الصالحة هي \"sqlite\" و \"mysql"
"\" و \"postgres\"."

#: dbmigrator.cpp:504
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "خطأ: فشل في ضبط خادم قاعدة البيانات المصدر."

#: dbmigrator.cpp:509
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "محركات قاعدة البيانات المصدر والوجهة هي نفسها."

#: dbmigrator.cpp:516
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"خطأ: فشل في الحصول على المسار إلى ملف أو دليل بيانات قاعدة البيانات المصدر."

#: dbmigrator.cpp:523
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "خطأ: فشل في تكوين خادم قاعدة البيانات الجديد."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr "خطأ: فشل في فتح قاعدة البيانات الموجودة لترحيل البيانات منها."

#: dbmigrator.cpp:534
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "خطأ: فشل في فتح قاعدة بيانات جديدة لترحيل البيانات إليها."

#: dbmigrator.cpp:539
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "تشغيل أمر التحقق من اتساق نظام الملفات fsck على قاعدة البيانات المصدر"

#: dbmigrator.cpp:558
#, kde-format
msgctxt "@info:status %2 is error message"
