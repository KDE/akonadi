#: dbmigrator.cpp:437
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "إيقاف خدمة أكونادي..."

#: dbmigrator.cpp:440
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "خطأ: أنتهت المهلة أثناء انتظار توقف أكونادي."

#: dbmigrator.cpp:447
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "خطأ: تعذر الحصول على قفل DBus لأكونادي."

#: dbmigrator.cpp:456
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "بدء خدمة أكونادي..."

#: dbmigrator.cpp:469
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "اكتمل التحقق من اتساق نظام الملفات fsck لقاعدة البيانات"

#: dbmigrator.cpp:483
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"محرك قاعدة البيانات \"%1\" غير صالح - القيم الصالحة هي \"sqlite\" و \"mysql"
"\" و \"postgres\"."

#: dbmigrator.cpp:505
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "خطأ: فشل في ضبط خادم قاعدة البيانات المصدر."

#: dbmigrator.cpp:510
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "محركات قاعدة البيانات المصدر والوجهة هي نفسها."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"خطأ: فشل في الحصول على المسار إلى ملف أو دليل بيانات قاعدة البيانات المصدر."

#: dbmigrator.cpp:524
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "خطأ: فشل في تكوين خادم قاعدة البيانات الجديد."

#: dbmigrator.cpp:530
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr "خطأ: فشل في فتح قاعدة البيانات الموجودة لترحيل البيانات منها."

#: dbmigrator.cpp:535
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "خطأ: فشل في فتح قاعدة بيانات جديدة لترحيل البيانات إليها."

#: dbmigrator.cpp:540
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "تشغيل أمر التحقق من اتساق نظام الملفات fsck على قاعدة البيانات المصدر"

#: dbmigrator.cpp:559
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "خطأ: فشل في إزالة دليل قاعدة البيانات المؤقتة %1: %2"

#: dbmigrator.cpp:566
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "أخذ نسخة احتياطية لقاعدة البيانات الأصلية..."

#: dbmigrator.cpp:574
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "استعادة قاعدة البيانات من النسخة الاحتياطية %1 إلى %2"

#: dbmigrator.cpp:576
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr "خطأ: فشل في استعادة قاعدة البيانات من النسخة الاحتياطية: %1"

#: dbmigrator.cpp:608 dbmigrator.cpp:619
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "حدث خطأ أثناء ترحيل الجدول %1"

#: dbmigrator.cpp:638
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr ""
"دليل النسخ الاحتياطي موجود بالفعل. هل تريد استبدال النسخة الاحتياطية السابقة؟"

#: dbmigrator.cpp:643
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "لا يمكن المتابعة بدون النسخ الاحتياطي. قطعت عملية الترحيل."

#: dbmigrator.cpp:648
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "فشل في إزالة دليل النسخ الاحتياطي السابق."

#: dbmigrator.cpp:661
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "فشل في نقل قاعدة البيانات إلى موقع النسخ الاحتياطي: %1"

#: dbmigrator.cpp:675
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "ملف النسخ الاحتياطي %1 موجود بالفعل. هل أكتب فوقها؟"

#: dbmigrator.cpp:677
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "لا يمكن المتابعة بدون النسخ الاحتياطي. قطعت عملية الترحيل."

#: dbmigrator.cpp:683
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "خطأ: فشل في إزالة ملف النسخ الاحتياطي الموجود %1: %2"

#: dbmigrator.cpp:690
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "فشل في عمل نسخة احتياطية لتكوين خادم أكونادي: %1"

#: dbmigrator.cpp:707
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr "خطأ: فشل في نقل قاعدة البيانات التي تم ترحيلها إلى الموقع الأساسي: %1"

#: dbmigrator.cpp:726
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "خطأ: فشل في إزالة akonadiserverrc الأصلي: %1"

#: dbmigrator.cpp:732
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr "خطأ: فشل في نقل akonadiserverrc الجديد إلى الموقع الأساسي: %1"

#: dbmigrator.cpp:823
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr "خطأ: فشل في تحديث قيمة الإضافة التلقائية للجدول %1"

#: dbmigrator.cpp:828
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "تحسين الجدول %1..."

#: dbmigrator.cpp:831
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "خطأ: فشل في تحسين الجدول %1"

