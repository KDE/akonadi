#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr ""

#: dbmigrator.cpp:484
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Motore di banca dati «%1» non valido - i valori validi sono «sqlite», "
"«mysql» e «postgres»."

#: dbmigrator.cpp:506
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Errore: impossibile configure il server di banca dati sorgente."

#: dbmigrator.cpp:511
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "I motori delle banche dati sorgente e destinazione sono gli stessi."

#: dbmigrator.cpp:518
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Errore: impossibile ottenere il percorso al file o cartella di dati della "
"banca dati sorgente."

#: dbmigrator.cpp:525
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Errore: impossibile configurare il nuovo server di banca dati."

