#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Napaka: preseljene zbirke podatkov ni bilo mogoče premakniti na primarno "
"lokacijo: %1"

#: dbmigrator.cpp:727
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Napaka: izvirne akonadiserverrc ni uspelo odstraniti: %1"

#: dbmigrator.cpp:733
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Napaka: ni uspelo premakniti nove akonadiserverrc na primarno lokacijo: %1"

