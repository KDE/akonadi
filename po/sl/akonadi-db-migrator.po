#: dbmigrator.cpp:449
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "Zaustavljanje storitve Akonadi..."

#: dbmigrator.cpp:452
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "Napaka: časovna omejitev ob čakanju, da se Akonadi zaustavi."

#: dbmigrator.cpp:459
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "Napaka: ni bilo mogoče pridobiti zaklepanja DBus za Akonadi."

#: dbmigrator.cpp:468
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Zaganjanje storitve Akonadi..."

#: dbmigrator.cpp:481
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Baza podatkov fsck končana"

#: dbmigrator.cpp:495
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Neveljaven stroj zbirke podatkov \"%1\" – veljavne vrednosti so \"sqlite\", "
"\"mysql\" in \"postgres\"."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Napaka: ni uspelo konfigurirati izvornega strežnika baze podatkov."

#: dbmigrator.cpp:522
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "Podatkovni stroj izvorne in ciljne baze podatkov je isti."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Napaka: ni bilo mogoče pridobiti poti do podatkovne datoteke ali imenika "
"izvorne baze podatkov."

#: dbmigrator.cpp:536
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Napaka: konfiguracija novega strežnika baze podatkov ni uspela."

#: dbmigrator.cpp:542
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr "Napaka: ni uspelo odpreti obstoječe baze podatkov za selitev podatkov."

#: dbmigrator.cpp:547
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr "Napaka: ni uspelo odpreti nove baze podatkov za selitev podatkov."

#: dbmigrator.cpp:552
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "Izvajanje fsck na izvorni bazi podatkov"

#: dbmigrator.cpp:571
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "Napaka: ni uspelo odstraniti začasnega imenika baze podatkov %1: %2"

#: dbmigrator.cpp:578
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "Varnostno kopiranje izvirne baze podatkov..."

#: dbmigrator.cpp:586
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "Obnavljanje baze podatkov iz varnostne kopije %1 v %2"

#: dbmigrator.cpp:588
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr "Napaka: obnova baze podatkov iz varnostne kopije ni uspela: %1"

#: dbmigrator.cpp:620 dbmigrator.cpp:631
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "Med selitvijo tabele %1 je prišlo do napake"

#: dbmigrator.cpp:650
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr ""
"Imenik za varnostno kopijo že obstaja. Ali želite prepisati prejšnjo "
"varnostno kopijo?"

#: dbmigrator.cpp:655
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Brez varnostne kopije ni mogoče nadaljevati. Selitev prekinjena."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Prejšnjega imenika za varnostno kopijo ni bilo mogoče odstraniti."

#: dbmigrator.cpp:673
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "Baze podatkov ni bilo mogoče premakniti na mesto varnostne kopije: %1"

#: dbmigrator.cpp:687
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "Varnostna datoteka %1 že obstaja. Naj jo prepišem?"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Brez varnostne kopije ni mogoče nadaljevati. Selitev prekinjena."

#: dbmigrator.cpp:695
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "Napaka: ni uspelo odstraniti obstoječe varnostne kopije %1: %2"

#: dbmigrator.cpp:702
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr "Varnostno kopiranje konfiguracije strežnika Akonadi ni uspelo: %1"

#: dbmigrator.cpp:719
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Napaka: preseljene zbirke podatkov ni bilo mogoče premakniti na primarno "
"lokacijo: %1"

#: dbmigrator.cpp:738
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Napaka: izvirne akonadiserverrc ni uspelo odstraniti: %1"

#: dbmigrator.cpp:744
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Napaka: ni uspelo premakniti nove akonadiserverrc na primarno lokacijo: %1"

#: dbmigrator.cpp:835
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr ""
"Napaka: posodobitev vrednosti samodejnega povečanja za tabelo %1 ni uspela"

#: dbmigrator.cpp:840
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "Optimizacija tabele %1..."

#: dbmigrator.cpp:843
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Napaka: optimizacija tabele %1 ni uspela"

