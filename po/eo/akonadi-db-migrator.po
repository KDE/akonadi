#, kde-format
msgctxt "@info:shell"
msgid "Invalid target engine: %1."
msgstr "Nevalida celmotoro: %1."

#: main.cpp:106
#, kde-format
msgctxt "@info:status"
msgid "Migration completed successfully."
msgstr "Migrado finiĝis sukcese."

#: main.cpp:108
#, kde-format
msgctxt "@info:status"
msgid "Migration failed."
msgstr "Migrado malsukcesis."

