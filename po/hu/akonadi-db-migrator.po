msgctxt "@info:shell"
msgid "Invalid target engine: %1."
msgstr "Érvénytelen cél alrendszer: %1."

#: main.cpp:106
#, kde-format
msgctxt "@info:status"
msgid "Migration completed successfully."
msgstr "A költöztetés sikeresen befejeződött."

#: main.cpp:108
#, kde-format
msgctxt "@info:status"
msgid "Migration failed."
msgstr "A költöztetés nem sikerült."

#: main.cpp:113
#, kde-format
msgctxt "@info:progress"
msgid "Migrating table %1 (%2/%3)..."
msgstr "A(z) %1 tábla költöztetése (%2/%3)…"

#: main.cpp:127
