msgid "Your emails"
msgstr "ulysses@fsf.hu"

#: dbmigrator.cpp:449
#, kde-format
msgctxt "@info:status"
msgid "Stopping Akonadi service..."
msgstr "Akonadi szolgáltatás leállítása…"

#: dbmigrator.cpp:452
#, kde-format
msgctxt "@info:status"
msgid "Error: timeout while waiting for Akonadi to stop."
msgstr "Hiba: időtúllépés az Akonadi leállására várva."

#: dbmigrator.cpp:459
#, kde-format
msgctxt "@info:status"
msgid "Error: couldn't acquire DBus lock for Akonadi."
msgstr "Hiba: nem sikerült megszerezni a DBus zárat az Akonadihoz."

#: dbmigrator.cpp:468
#, kde-format
msgctxt "@info:status"
msgid "Starting Akonadi service..."
msgstr "Akonadi szolgáltatás indítása…"

#: dbmigrator.cpp:481
#, kde-format
msgctxt "@info:status"
msgid "Database fsck completed"
msgstr "Az adatbázis fájlrendszer-ellenőrzése befejeződött"

#: dbmigrator.cpp:495
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid database engine \"%1\" - valid values are \"sqlite\", \"mysql\" and "
"\"postgres\"."
msgstr ""
"Érvénytelen adatbázis-alrendszer: „%1” - az érvényes értékek a következők: "
"„sqlite”, „mysql” és „postgres”."

#: dbmigrator.cpp:517
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure source database server."
msgstr "Hiba: nem sikerült beállítani a forrás adatbázis-kiszolgálót."

#: dbmigrator.cpp:522
#, kde-format
msgctxt "@info:shell"
msgid "Source and destination database engines are the same."
msgstr "A forrás és a cél adatbázis-alrendszer ugyanaz."

#: dbmigrator.cpp:529
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to obtain path to source database data file or directory."
msgstr ""
"Hiba: nem sikerült megszerezni a forrás adatbázis adatfájljának vagy "
"mappájának elérési útját."

#: dbmigrator.cpp:536
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to configure the new database server."
msgstr "Hiba: nem sikerült beállítani az új adatbázis-kiszolgálót."

#: dbmigrator.cpp:542
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open existing database to migrate data from."
msgstr ""
"Hiba: nem sikerült megnyitni a meglévő adatbázist az adatok "
"átköltöztetéséhez."

#: dbmigrator.cpp:547
#, kde-format
msgctxt "@info:shell"
msgid "Error: failed to open new database to migrate data to."
msgstr ""
"Hiba: nem sikerült megnyitni az új adatbázist az adatok átköltöztetéséhez."

#: dbmigrator.cpp:552
#, kde-format
msgctxt "@info:status"
msgid "Running fsck on the source database"
msgstr "Fájlrendszer-ellenőrzés fut a forrásadatbázison"

#: dbmigrator.cpp:571
#, kde-format
msgctxt "@info:status %2 is error message"
msgid "Error: failed to remove temporary database directory %1: %2"
msgstr "Hiba: nem sikerült törölni a(z) %1 átmeneti adatbázismappát: %2"

#: dbmigrator.cpp:578
#, kde-format
msgctxt "@info:shell"
msgid "Backing up original database..."
msgstr "Eredeti adatbázis biztonsági mentése…"

#: dbmigrator.cpp:586
#, kde-format
msgctxt "@info:status"
msgid "Restoring database from backup %1 to %2"
msgstr "Adatbázis visszaállítása a(z) %1 biztonsági mentésből ide: %2"

#: dbmigrator.cpp:588
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to restore database from backup: %1"
msgstr ""
"Hiba: nem sikerült visszaállítani az adatbázist a biztonsági mentésből: %1"

#: dbmigrator.cpp:620 dbmigrator.cpp:631
#, kde-format
msgctxt "@info:shell"
msgid "Error has occurred while migrating table %1"
msgstr "Hiba történt a(z) %1 tábla átköltöztetésekor"

#: dbmigrator.cpp:650
#, kde-format
msgctxt "@label"
msgid ""
"Backup directory already exists. Do you want to overwrite the previous "
"backup?"
msgstr ""
"A biztonsági mentés mappája már létezik. Szeretné felülírni az előző "
"biztonsági mentést?"

#: dbmigrator.cpp:655
#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Nem lehet folytatni biztonsági mentés nélkül. Költöztetés megszakítva."

#: dbmigrator.cpp:660
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "Nem sikerült törölni az előző biztonsági mentés mappáját."

#: dbmigrator.cpp:673
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
msgstr "Nem sikerült áthelyezni az adatbázist a biztonsági mentés helyére: %1"

#: dbmigrator.cpp:687
#, kde-format
msgctxt "@label"
msgid "Backup file %1 already exists. Overwrite?"
msgstr "A(z) %1 biztonsági mentés már létezik. Felülírja?"

#: dbmigrator.cpp:689
#, kde-format
msgctxt "@info:status"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "Nem lehet folytatni biztonsági mentés nélkül. Költöztetés megszakítva."

#: dbmigrator.cpp:695
#, kde-format
msgctxt "@info:status"
msgid "Error: Failed to remove existing backup file %1: %2"
msgstr "Hiba: nem sikerült eltávolítani a létező %1 biztonsági mentést: %2"

#: dbmigrator.cpp:702
#, kde-format
msgctxt "@info:status"
msgid "Failed to back up Akonadi Server configuration: %1"
msgstr ""
"Hiba: nem sikerült az Akonadi kiszolgáló beállításainak biztonsági mentése: "
"%1"

#: dbmigrator.cpp:719
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move migrated database to the primary location: %1"
msgstr ""
"Hiba: nem sikerült áthelyezni a költöztetett adatbázist az elsődleges "
"helyre: %1"

#: dbmigrator.cpp:738
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to remove original akonadiserverrc: %1"
msgstr "Hiba: nem sikerült eltávolítani az eredeti akonadisercerrc fájlt: %1"

#: dbmigrator.cpp:744
#, kde-format
msgctxt "@info:status %1 is error message"
msgid "Error: failed to move new akonadiserverrc to the primary location: %1"
msgstr ""
"Hiba: nem sikerült áthelyezni az új akonadiserverrc fájlt az elsődleges "
"helyre: %1"

#: dbmigrator.cpp:835
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to update autoincrement value for table %1"
msgstr ""
"Hiba: nem sikerült frissíteni a(z) %1 tábla automatikus növekményes értékét"

#: dbmigrator.cpp:840
#, kde-format
msgctxt "@info:status"
msgid "Optimizing table %1..."
msgstr "A(z) %1 tábla optimalizálása…"

#: dbmigrator.cpp:843
#, kde-format
msgctxt "@info:status"
msgid "Error: failed to optimize table %1"
msgstr "Hiba: nem sikerült a(z) %1 tábla optimalizálása"

#: main.cpp:31
#, kde-format
msgctxt ""
"Options for user to input to command line prompt, parenthesis indicate which "
"letter to type, capitalized option is default"
msgid " [(y)es/(N)o] "
msgstr " [(i)gen/(N)em] "

#: main.cpp:36 main.cpp:55
#, kde-format
msgctxt "Letter for option \"(y)es\" prompted from command line"
msgid "y"
msgstr "i"

#: main.cpp:49
#, kde-format
msgctxt ""
"Options for user to input to command line prompt, parenthesis indicate which "
"letter to type, capitalized option is default"
msgid " [(y)es/(N)o/(s)kip] "
msgstr " [(i)gen/(N)em/(k)ihagyás] "

#: main.cpp:56
#, kde-format
msgctxt "Letter for option \"(s)kip\" prompted from command line"
msgid "s"
msgstr "k"

