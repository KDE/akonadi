#, kde-format
msgid "Akonadi DB Migrator"
msgstr "Akonadi adatbázis-költöztető"

#: main.cpp:77
#, kde-format
msgid "(c) 2024 g10 Code GmbH"
msgstr "(c) g10 Code GmbH, 2024."

#: main.cpp:83
#, kde-format
msgid ""
"The new DB engine to use. Possible values are  \"sqlite\", \"mysql\" and "
"\"postgres\""
msgstr ""
"A használandó új adatbázis-alrendszer. A lehetséges értékek: „sqlite”, "
"„mysql” és „postgres”"

#: main.cpp:90
#, kde-format
msgctxt "@info:shell"
msgid "Invalid target engine: %1."
msgstr "Érvénytelen cél alrendszer: %1."

#: main.cpp:104
#, kde-format
msgctxt "@info:status"
msgid "Migration completed successfully."
msgstr "A költöztetés sikeresen befejeződött."

#: main.cpp:106
#, kde-format
msgctxt "@info:status"
msgid "Migration failed."
msgstr "A költöztetés nem sikerült."

#: main.cpp:111
#, kde-format
msgctxt "@info:progress"
msgid "Migrating table %1 (%2/%3)..."
msgstr "A(z) %1 tábla költöztetése (%2/%3)…"

#: main.cpp:125
#, kde-format
msgctxt "@info:progress"
msgid "%1%..."
msgstr "%1%…"
