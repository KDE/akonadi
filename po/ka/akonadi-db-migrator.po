#, kde-format
msgctxt "@info:shell"
msgid "Cannot proceed without backup. Migration interrupted."
msgstr "მარქაფის გარეშე ვერ გავაგრძელებ. მიგრაცია შეწყდა."

#: dbmigrator.cpp:648
#, kde-format
msgctxt "@info:shell"
msgid "Failed to remove previous backup directory."
msgstr "წინა მარქაფის საქაღალდის წაშლის მცდელობა ჩავარდა."

#: dbmigrator.cpp:661
#, kde-format
msgctxt "@info:shell"
msgid "Failed to move database to backup location: %1"
