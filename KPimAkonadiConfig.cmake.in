@PACKAGE_INIT@

set_and_check(AKONADI_DBUS_INTERFACES_DIR "@PACKAGE_AKONADI_DBUS_INTERFACES_INSTALL_DIR@")
set_and_check(AKONADI_INCLUDE_DIR "@PACKAGE_AKONADI_INCLUDE_DIR@")

# The directory where akonadi-xml.xsd and kcfg2dbus.xsl are installed
set(KF5Akonadi_DATA_DIR "@PACKAGE_KF5Akonadi_DATA_DIR@")

# set the directories
if(NOT AKONADI_INSTALL_DIR)
    set(AKONADI_INSTALL_DIR "@CMAKE_INSTALL_PREFIX@")
endif(NOT AKONADI_INSTALL_DIR)
include(CMakeFindDependencyMacro)
find_dependency(
    ECM
    "@KF_MIN_VERSION@"
)
find_dependency(
    KF6Config
    "@KF_MIN_VERSION@"
)
find_dependency(
    KF6ConfigWidgets
    "@KF_MIN_VERSION@"
)
find_dependency(
    KF6CoreAddons
    "@KF_MIN_VERSION@"
)
find_dependency(
    KF6ItemModels
    "@KF_MIN_VERSION@"
)
find_dependency(
    KF6XmlGui
    "@KF_MIN_VERSION@"
)

find_dependency(
    Qt6Core
    "@QT_REQUIRED_VERSION@"
)
find_dependency(
    Qt6DBus
    "@QT_REQUIRED_VERSION@"
)
find_dependency(
    Qt6Gui
    "@QT_REQUIRED_VERSION@"
)
find_dependency(
    Qt6Network
    "@QT_REQUIRED_VERSION@"
)
find_dependency(
    Qt6Widgets
    "@QT_REQUIRED_VERSION@"
)
find_dependency(
    Qt6Xml
    "@QT_REQUIRED_VERSION@"
)

if(BUILD_TESTING)
    # Link targets required by KF5AkonadiMacros.cmake
    find_dependency(
        Qt6Test
        "@QT_REQUIRED_VERSION@"
    )
endif()

include(${CMAKE_CURRENT_LIST_DIR}/KPim6AkonadiTargets.cmake)
include(${CMAKE_CURRENT_LIST_DIR}/KPimAkonadiMacros.cmake)

@PACKAGE_SETUP_AUTOMOC_VARIABLES@
